		<row><!-- Delta row-->
		    <entry align="left">
			<m:math>
			    <m:apply>
				<m:eq/>
				<m:apply>
				    <m:ci>δ</m:ci>
				    <m:ci>t</m:ci>
				</m:apply>
				<m:cn>0</m:cn>
			    </m:apply>
			</m:math> for
			<m:math><m:apply><m:neq/><m:ci>t</m:ci><m:cn>0</m:cn></m:apply></m:math>,
			<m:math>
			    <m:apply>
				<m:eq/>
				<m:apply>
				    <m:int/>
				    <m:bvar><m:ci>t</m:ci></m:bvar>
				    <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
				    <m:uplimit><m:infinity/></m:uplimit>
				    <m:apply>
				    	<m:ci>δ</m:ci>
					<m:ci>t</m:ci>
				    </m:apply>
			  	</m:apply>
				<m:cn>1</m:cn>
			    </m:apply>
			</m:math>
		    </entry>
		    <entry align="left">
		        <m:math>
			    <m:apply>
				<m:eq/>
				<m:apply>
				    <m:ci>δ</m:ci>
				    <m:ci>n</m:ci>
				</m:apply>
				<m:piecewise>
				    <m:piece>
				        <m:cn>1</m:cn>
					<m:apply><m:eq/><m:ci>n</m:ci><m:cn>0</m:cn></m:apply>
				    </m:piece>
				    <m:otherwise>
				        <m:cn>0</m:cn>
				    </m:otherwise>
				</m:piecewise>
			    </m:apply>
			</m:math>
		    </entry>
		   
		</row><!--end row 1-->
