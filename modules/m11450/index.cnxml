		<row>
		    <entry align="left">Energy
		    <m:math>
		      <m:apply>
		          <m:eq/>
			  <m:ci><m:msub><m:mi>E</m:mi><m:mi>a</m:mi></m:msub></m:ci>
			  <m:apply>
			      <m:int/>
			      <m:bvar><m:ci>t</m:ci></m:bvar>
			      <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
			      <m:uplimit><m:infinity/></m:uplimit>
			      <m:apply>
			          <m:power/>
				  <m:apply>
				      <m:abs/>
				      <m:apply>
				          <m:ci>x</m:ci>
				          <m:ci>t</m:ci>
				      </m:apply>
				  </m:apply>
