        </entry>
	<entry align="left">Power
	    <m:math>
		<m:apply>
		    <m:eq/>
		    <m:ci><m:msub><m:mi>P</m:mi><m:mi>d</m:mi></m:msub></m:ci>
		    <m:apply>
			<m:times/>
			<m:apply>
			    <m:divide/>
		            <m:cn>1</m:cn>
			    <m:ci>N</m:ci>
			</m:apply>
			<m:apply>
			    <m:sum/>
		            <m:bvar><m:ci>n</m:ci></m:bvar>
			    <m:lowlimit><m:ci><m:msub><m:mi>N</m:mi><m:mn>1</m:mn></m:msub></m:ci></m:lowlimit>
			    <m:uplimit>
				<m:apply>
				    <m:minus/>
					<m:apply>
					    <m:plus/>					     
					    <m:ci><m:msub><m:mi>N</m:mi><m:mn>1</m:mn></m:msub></m:ci>
					    <m:ci>N</m:ci>
					</m:apply>
					<m:cn>1</m:cn>
				</m:apply>
			    </m:uplimit>
			    <m:apply>
				<m:power/>
				      <m:apply>
				          <m:abs/>
					  <m:apply>
					      <m:ci>x</m:ci>
					      <m:ci>n</m:ci>
					  </m:apply>
				      </m:apply>
				      <m:cn>2</m:cn>
				  </m:apply>
			      </m:apply>
			  </m:apply>
		      </m:apply>
		  </m:math>
		    </entry>
		</row>
		<row>
		    <entry align="left"/>
		    <entry align="left"/>
		</row>
		<row>
		    <entry align="left">Convol.
			<m:math>
			    <m:apply>
				<m:eq/>
				<m:apply>
				    <m:ci>y</m:ci>
				    <m:ci>t</m:ci>
				</m:apply>
				<m:apply>
				    <m:int/>
				    <m:bvar><m:ci>τ</m:ci></m:bvar>
				    <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
				    <m:uplimit><m:infinity/></m:uplimit>
				    <m:apply>
					<m:times/>
					<m:apply>
					    <m:ci>x</m:ci>
					    <m:ci>τ</m:ci> 
					</m:apply>
					<m:apply>
					    <m:ci>h</m:ci>
					    <m:apply>
						<m:minus/>
						<m:ci>t</m:ci>
						<m:ci>τ</m:ci> 
					    </m:apply>
					</m:apply>
				    </m:apply>
				</m:apply>
			    </m:apply>
			</m:math>
		    </entry>
		    
		    <entry align="left">Convol.
		        <m:math>
			    <m:apply>
				<m:eq/>
				<m:apply>
				    <m:ci>y</m:ci>
				    <m:ci>n</m:ci>
				</m:apply>
				<m:apply>
				    <m:sum/>
				    <m:bvar><m:ci>k</m:ci></m:bvar>
				    <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
				    <m:uplimit><m:infinity/></m:uplimit>
				    <m:apply>
					<m:times/>
					<m:apply>
					    <m:ci>x</m:ci>
					    <m:ci>k</m:ci> 
					</m:apply>
					<m:apply>
					    <m:ci>h</m:ci>
					    <m:apply>
						<m:minus/>
						<m:ci>n</m:ci>
						<m:ci>k</m:ci> 
					    </m:apply>
					</m:apply>
				    </m:apply>
				</m:apply>
			    </m:apply>
			</m:math>
		    </entry>
		</row>
		<row>
		    <entry align="left"/>
		    <entry align="left"/>
		</row>		
		<row>
		    <entry align="left">Fourier Transformation</entry>
		    <entry align="left">Discrete Time Fourier Transform</entry>
		</row>
		<row>
		  <entry align="left">
		      <m:math>
		      <m:apply>
			<m:eq/>
			<m:apply>
			    <m:ci>X</m:ci>
			    <m:apply>
				<m:times/>
				<m:imaginaryi/>
				<m:ci>Ω</m:ci>
			    </m:apply>
			</m:apply>
						    			    
			    <m:apply>
			        <m:int/>
			        <m:bvar><m:ci>t</m:ci></m:bvar>
			        <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
			        <m:uplimit><m:infinity/></m:uplimit>
			        <m:apply>
				    <m:times/>
				    <m:apply>
				        <m:ci>x</m:ci>
					<m:ci>t</m:ci>
				    </m:apply>
				    <m:apply>
				        <m:exp/>
					<m:apply>
                                            <m:minus/>
		                            <m:apply>
                                               <m:times/>     
                                               <m:imaginaryi/>
                                               <m:ci>Ω</m:ci>
					       <m:ci>t</m:ci>
                                            </m:apply>
					</m:apply>
				    </m:apply>
				</m:apply>
			    </m:apply>
			   
			</m:apply>
		    </m:math>

		  </entry>
		  <entry align="left">
		      <m:math>
	  <m:apply>
	    <m:eq/>
	    <m:apply>
	      <m:ci>X</m:ci>
	      <m:apply>
		<m:exp/>
		<m:apply>
		  <m:times/>
		  <m:imaginaryi/>
		  <m:ci>ω</m:ci>
		</m:apply>
	      </m:apply>
	    </m:apply>
	    <m:apply>
	      <m:sum/>
	      <m:bvar><m:ci>n</m:ci></m:bvar>
	      
	      <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
	      <m:uplimit><m:infinity/></m:uplimit>
	      <m:apply>
		<m:times/>
		<m:apply>
		  <m:ci>x</m:ci>
		  <m:ci>n</m:ci>
		</m:apply>
		<m:apply>
		  <m:exp/>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:ci>ω</m:ci>
		      <m:ci>n</m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:apply>
	    </m:apply>
	  </m:apply>
	</m:math>   
	</entry>
	</row>
	<row>
	    <entry align="left"/>
	    <entry align="left"/>
	</row>
		<row>
		    <entry align="left">Inverse Fourier Transform</entry>
		    <entry align="left">Inverse DTFT</entry>
		</row>		
	 <row>
	     <entry align="left">
	     
	     <m:math>
	     <m:apply>
	         <m:eq/>
	         <m:apply>
		     <m:ci>x</m:ci>
		     <m:ci>t</m:ci>
		 </m:apply>
		 <m:apply>
	      	     <m:times/>
		     <m:apply>
		         <m:divide/>
			 <m:cn>1</m:cn>
			 <m:apply>
			     <m:times/>
			     <m:cn>2</m:cn>
			     <m:pi/>
			 </m:apply>
		      </m:apply>
		      
		      <m:apply>
		      <m:int/>
		      <m:bvar><m:ci>Ω</m:ci></m:bvar>
	      <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
	      <m:uplimit><m:infinity/></m:uplimit>
	      <m:apply>
	      <m:times/>
		<m:apply>
		  <m:ci>X</m:ci>
		  <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:ci>ω</m:ci>
		  </m:apply>
		</m:apply>
		<m:apply>
		  <m:exp/>
		  <m:apply>
		    <m:plus/>
		    <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:ci>Ω</m:ci>
		      <m:ci>t</m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
		</m:apply>
	      </m:apply>
	    </m:apply>
	  </m:apply>
	</m:math>
	     
	     </entry>
	     <entry align="left">
	  <m:math>
	  <m:apply>
	    <m:eq/>
	    <m:apply>
	      <m:ci>x</m:ci>
	      <m:ci>n</m:ci>
	    </m:apply>

	    <m:apply>
	      	     <m:times/>
		     <m:apply>
		         <m:divide/>
			 <m:cn>1</m:cn>
			 <m:apply>
			     <m:times/>
			     <m:cn>2</m:cn>
			     <m:pi/>
			 </m:apply>
		      </m:apply>
	      <m:apply>
	      <m:int/>
	      <m:bvar><m:ci>ω</m:ci></m:bvar>
	      <m:lowlimit><m:apply><m:minus/><m:pi/></m:apply></m:lowlimit>
	      <m:uplimit><m:pi/></m:uplimit>
	      <m:apply>
	      <m:times/>
		<m:apply>
		  <m:ci>X</m:ci>
		  <m:apply>
		    <m:exp/>
		    <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:ci>ω</m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
		<m:apply>
		  <m:exp/>
		  <m:apply>
		    <m:plus/>
		    <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:ci>ω</m:ci>
		      <m:ci>n</m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:apply>
	      </m:apply>
	    </m:apply>
	  </m:apply>
	</m:math>
	</entry>
	</row>
	<row>
	    <entry align="left"/>
	    <entry align="left"/>
	</row>
	<row>
	    <entry align="left">Fourier coeffecients</entry>
	    <entry align="left">Discrete Fourier Transform</entry>
	</row>
	<row>
	    <entry align="left">
	        <m:math>
		<m:apply>
		    <m:eq/>
		    <m:ci><m:msub><m:mi>α</m:mi><m:mi>k</m:mi></m:msub></m:ci>
		    <m:apply>
		    <m:times/>
			<m:apply>
			    <m:divide/>
			    <m:cn>1</m:cn>
			    <m:ci><m:msub><m:mi>T</m:mi><m:mn>0</m:mn></m:msub></m:ci>
			</m:apply>
		    <m:apply>
			<m:int/>
			<m:bvar><m:ci>t</m:ci></m:bvar>
			<m:lowlimit><m:cn>0</m:cn></m:lowlimit>
			<m:uplimit><m:ci><m:msub><m:mi>T</m:mi><m:mn>0</m:mn></m:msub></m:ci></m:uplimit>
			        <m:apply>
				    <m:times/>
				    <m:apply>
				        <m:ci>x</m:ci>
					<m:ci>t</m:ci>
				    </m:apply>
				    <m:apply>
				        <m:exp/>
					<m:apply>
                                            <m:minus/>
		                            <m:apply>
                                               <m:times/>     
                                               <m:imaginaryi/>
					       <m:ci>k</m:ci>
                                               <m:ci><m:msub><m:mi>Ω</m:mi><m:mn>0</m:mn></m:msub></m:ci>
					       <m:ci>t</m:ci>
                                            </m:apply>
					</m:apply>
				    </m:apply>
				</m:apply>
			    </m:apply>
			   </m:apply>
			   </m:apply>
		    </m:math>	
	    </entry>

		  <entry align="left">
		       <m:math>
	  <m:apply>
	    <m:eq/>
	    <m:apply>
	      <m:ci>X</m:ci>
	      <m:ci>k</m:ci>
	    </m:apply>
	    <m:apply>
	      <m:sum/>
	      <m:bvar><m:ci>n</m:ci></m:bvar>
	      <m:lowlimit><m:cn>0</m:cn></m:lowlimit>
	      <m:uplimit><m:apply><m:minus/><m:ci>N</m:ci><m:cn>1</m:cn></m:apply></m:uplimit>
	      <m:apply>
		<m:times/>
		<m:apply>
		  <m:ci>x</m:ci>
		  <m:ci>n</m:ci>
		</m:apply>
		<m:apply>
		  <m:exp/>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:times/>
		      <m:imaginaryi/>
		      <m:apply>
		        <m:divide/>
			<m:apply>
			  <m:times/>
		          <m:cn>2</m:cn>
		          <m:pi/>
			</m:apply>
			<m:ci>N</m:ci>
		      </m:apply>
		      <m:ci>k</m:ci>
		      <m:ci>n</m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:apply>
	    </m:apply>
	  </m:apply>
	</m:math>   
	</entry>	
		
	</row>
	<row>
	    <entry align="left"/>
	    <entry align="left"/>
	</row>		
	<row>
	    <entry align="left">Series expansion</entry>
	    <entry align="left">Inverse DFT</entry>
	</row>		
	<row>
	<entry align="left">
	    <m:math>
	    <m:apply>
		<m:eq/>
		<m:apply>
		    <m:ci>x</m:ci>
		    <m:ci>t</m:ci>
		</m:apply>
		<m:apply>
		    <m:sum/>
		    <m:bvar><m:ci>k</m:ci></m:bvar>
		    <m:lowlimit><m:apply><m:minus/><m:infinity/></m:apply></m:lowlimit>
		    <m:uplimit><m:apply><m:infinity/></m:apply></m:uplimit>
		    <m:apply>
		        <m:times/>
			<m:ci><m:msub><m:mi>α</m:mi><m:mi>k</m:mi></m:msub></m:ci>
			<m:apply>
			    <m:exp/>
			    <m:apply>
				<m:times/>
				<m:imaginaryi/>
				<m:ci>k</m:ci>
				<m:ci><m:msub><m:mi>Ω</m:mi><m:mn>0</m:mn></m:msub></m:ci>
				<m:ci>t</m:ci>
			    </m:apply>
		        </m:apply>
		    </m:apply>
		</m:apply>
	    </m:apply>
	    </m:math>
	</entry>
        
	<entry align="left">
	    <m:math>
		<m:apply>
		    <m:eq/>
		    <m:apply>
			<m:ci>x</m:ci>
			<m:ci>n</m:ci>
		    </m:apply>
		    <m:apply>
			<m:times/>
			<m:apply>
			    <m:divide/>
			    <m:cn>1</m:cn>
			    <m:ci>N</m:ci>
			</m:apply>

			<m:apply>
			    <m:sum/>
			    <m:bvar><m:ci>k</m:ci></m:bvar>
			    <m:lowlimit><m:cn>0</m:cn></m:lowlimit>
			    <m:uplimit><m:apply><m:minus/><m:ci>N</m:ci><m:cn>1</m:cn></m:apply></m:uplimit>

			    <m:apply>
			        <m:times/>
			        <m:apply>
				    <m:ci>X</m:ci>
				    <m:ci>k</m:ci>
				</m:apply>
				<m:apply>
				    <m:exp/>
				    <m:apply>
				        <m:times/>
					<m:imaginaryi/>
					<m:apply>
					    <m:divide/>
					    <m:apply>
						<m:times/>
						<m:cn>2</m:cn>
						<m:pi/>
					     </m:apply>
					     <m:ci>N</m:ci>
					</m:apply>
					<m:ci>k</m:ci>
					<m:ci>n</m:ci>
				    </m:apply>
				</m:apply>
			    </m:apply>
			</m:apply>
		    </m:apply>
		</m:apply>
	    </m:math>   
	</entry>	
	</row>
	<row>
	    <entry align="left"/>
	    <entry align="left"/>
	</row>
	<row>
	    <entry align="left">Parseval</entry>
	    <entry align="left">Parseval</entry>
	</row>
	<row>
	    <entry align="left">
		<m:math>
		    <m:apply>
		        <m:eq/>
			<m:apply>
		            <m:int/>
			    <m:bvar><m:ci>t</m:ci></m:bvar>
			    <m:lowlimit>
			   	<m:ci><m:msub><m:mi>T</m:mi><m:mn>1</m:mn></m:msub></m:ci>
			    </m:lowlimit>
			    <m:uplimit>
			        <m:ci><m:msub><m:mi>T</m:mi><m:mn>0</m:mn></m:msub></m:ci>
			    </m:uplimit>
			    <m:apply>
				<m:power/>
				<m:apply>
				    <m:abs/>
				    <m:apply>
					<m:ci>x</m:ci>
					<m:ci>t</m:ci>
				    </m:apply>
				</m:apply>
				<m:cn>2</m:cn>
			    </m:apply>
			  </m:apply>
		      
		          <m:apply>
		      	      <m:sum/>
			      <m:bvar><m:ci>k</m:ci></m:bvar>
			      <m:lowlimit><m:cn>1</m:cn></m:lowlimit>
			      <m:uplimit><m:infinity/></m:uplimit>
			      <m:apply>
			      <m:power/>
			      <m:apply>
			          <m:abs/>
				  <m:apply>
		 		      <m:ci><m:msub><m:mi>α</m:mi><m:mi>k</m:mi></m:msub></m:ci>
				  </m:apply>
			      </m:apply>
			      <m:cn>2</m:cn>
			   </m:apply>
		      </m:apply>
		      </m:apply>
		  </m:math>
	    </entry>
	    <entry align="left">
		<m:math>
		    <m:apply>
		        <m:eq/>
			<m:apply>
			    <m:times/>
			    <m:apply><m:divide/><m:cn>1</m:cn><m:ci>N</m:ci></m:apply>
			<m:apply>
			    <m:sum/>
			    <m:bvar><m:ci>k</m:ci></m:bvar>
			    <m:lowlimit><m:cn>0</m:cn></m:lowlimit>
			    <m:uplimit><m:apply><m:minus/><m:ci>N</m:ci><m:cn>1</m:cn></m:apply></m:uplimit>
			    <m:apply>
			        <m:power/>
				<m:apply>
				    <m:abs/>
				    <m:apply>
				        <m:ci>X</m:ci>
				        <m:ci>k</m:ci>
				    </m:apply>
				 </m:apply>
				 <m:cn>2</m:cn>
			      </m:apply>
			      </m:apply>
			  </m:apply>
			<m:apply>
			    <m:sum/>
			    <m:bvar><m:ci>n</m:ci></m:bvar>
			    <m:lowlimit><m:cn>0</m:cn></m:lowlimit>
			    <m:uplimit><m:apply><m:minus/><m:ci>N</m:ci><m:cn>1</m:cn></m:apply></m:uplimit>
			    <m:apply>
			        <m:power/>
				<m:apply>
				    <m:abs/>
				    <m:apply>
				        <m:ci>x</m:ci>
				        <m:ci>n</m:ci>
				    </m:apply>
				 </m:apply>
				 <m:cn>2</m:cn>
			      </m:apply>
			  </m:apply>
		      </m:apply>
		  </m:math>
	    </entry>
	</row>


